ia only screen and (max-width: 992px) {\n        .event{\n            .holder{\n                .description-holder{\n                    width: 90%;\n                    text-align: center;\n                    margin-left: 0em;\n                    margin: 0 auto;\n                }\n                .content-holder{\n                    text-align: center;\n                }\n            }\n        }\n    }\n        /** md **/\n    @media only screen and (max-width: 768px) {\n        .event{\n            .holder{\n                .image-holder{\n                    width: 250px;\n                }\n            }\n        }\n    }\n</style>\n\n"]}]}